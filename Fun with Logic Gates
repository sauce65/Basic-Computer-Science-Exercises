# Simple program demonstrating knowledge of logic gates, may play around with this more to get some meta-computing going for fun!

def NOT(z):
    if z == 1:
        return 0
    else:
        return 1

def OR(x, y):
    if x == 1 or y == 1:
        return 1
    else:
        return 0

def AND(x, y):
    if x == 1 and y == 1:
        return 1
    else:
        return 0

def NOR(x, y):
    z = OR(x, y)
    return NOT(z)

def NAND(x, y):
    z = AND(x, y)
    return NOT(z)
